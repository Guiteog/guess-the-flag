    {
        "id": "013a246f0cd65995",
        "type": "function",
        "z": "f0c65390ea861234",
        "name": "function 1",
        "func": "let dados;\n\n// Tenta processar os dados do arquivo\ntry {\n    let jsonString = msg.payload.trim(); // Remove espaços em branco extras\n    let parsedData = JSON.parse(jsonString); // Converte de volta para objeto\n\n    if (!Array.isArray(parsedData)) {\n        parsedData = [parsedData]; // Se não for array, transforma em array\n    }\n\n    // Junta todos os dados em um único array\n    dados = parsedData.reduce((acc, item) => {\n        if (item.nome) {\n            acc.push({ nome: item.nome });\n        }\n        return acc;\n    }, []);\n    \n} catch (e) {\n    dados = []; // Caso de erro, cria um array vazio\n}\n\n// Adiciona o nome enviado pela URL\nlet nome = msg.req.query.nome;\nif (nome) {\n    dados.push({ nome: nome }); // Adiciona o novo nome\n}\n\n// Atualiza msg.payload com o JSON atualizado\nmsg.payload = JSON.stringify(dados, null, 2); // Formatação legível\n\nreturn msg;\n",
